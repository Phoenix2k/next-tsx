{
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "classnames": "^2.2.6",
    "emotion-theming": "^10.0.27",
    "next": "^9.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0"
  },
  "description": "Next.js build with TypeScript and TSX support",
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@cypress/webpack-preprocessor": "^4.1.1",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.9.0",
    "@types/react": "^16.9.18",
    "@types/react-dom": "^16.9.5",
    "@types/react-test-renderer": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-emotion": "^10.0.27",
    "cypress": "^3.8.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-markdown": "^1.0.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "husky": "^4.1.0",
    "jest-emotion": "^10.0.27",
    "jsdom": "^16.0.1",
    "jsdom-global": "^3.0.2",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "remark": "^11.0.2",
    "remark-cli": "^7.0.1",
    "remark-lint": "^6.0.5",
    "remark-preset-lint-markdown-style-guide": "^2.1.3",
    "remark-preset-lint-recommended": "^3.0.3",
    "start-server-and-test": "^1.10.6",
    "stylelint": "^13.0.0",
    "stylelint-config-standard": "^19.0.0",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": "~12.13.0",
    "npm": "~6.13.0"
  },
  "name": "next-tsx",
  "private": true,
  "scripts": {
    "build": "next build",
    "cypress:ci": "CYPRESS_CI=true cypress run --config-file __tests__/cypress/cypress.json --config video=false",
    "cypress:run": "cypress run --config-file __tests__/cypress/cypress.json",
    "dev": "next",
    "export": "npm run build && next export -o dist",
    "lint": "npm run lint:css && npm run lint:markdown && npm run lint:source && npm run lint:tests",
    "lint:css": "stylelint --ignore-path '.eslintignore' '**/*.{js,ts,tsx}'",
    "lint:fix": "npm run lint:fix:markdown && npm run lint:fix:source && npm run lint:fix:tests",
    "lint:fix:markdown": "remark --rc-path '.remarkrc.json' '.' --output",
    "lint:fix:source": "eslint --config '.eslintrc.json' --fix '**/*.{js,json,md,ts,tsx}'",
    "lint:fix:tests": "eslint --config '__tests__/.eslintrc.json' --ignore-path '__tests__/.eslintignore' --fix '**/*.{js,json,md,ts,tsx}'",
    "lint:markdown": "remark --rc-path '.remarkrc.json' '.'",
    "lint:source": "eslint --config '.eslintrc.json' '**/*.{js,json,md,ts,tsx}'",
    "lint:tests": "eslint --config '__tests__/.eslintrc.json' --ignore-path '__tests__/.eslintignore' '__tests__/**/*.{js,json,md,ts,tsx}'",
    "prettier": "prettier --config '.prettierrc.json' --ignore-path '.gitignore' --write '**/*.{js,json,ts,tsx}'",
    "prettier:check": "prettier --check --config '.prettierrc.json' --ignore-path '.gitignore' '**/*.{js,json,ts,tsx}'",
    "start": "next start",
    "test": "npm run test:unit && npm run test:e2e",
    "test:ci": "jest --config '__tests__/jest/jest.config.js' && next build && start-test start 3000 cypress:ci",
    "test:e2e": "next build && start-test start 3000 cypress:run",
    "test:unit": "jest --config '__tests__/jest/jest.config.js' --coverage",
    "test:update": "jest --config '__tests__/jest/jest.config.js' --coverage --updateSnapshot"
  },
  "version": "0.1.0"
}
